# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

fresh gitconfig --file=$HOME/.config/git/config

fresh rofi.rasi --file=$HOME/.config/rofi/config.rasi

fresh i3status-rs --file=$HOME/.config/i3status-rs/config.toml
